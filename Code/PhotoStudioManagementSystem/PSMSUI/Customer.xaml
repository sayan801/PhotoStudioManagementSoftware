<Window x:Class="PSMSUI.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Window1" Height="375" Width="551">
    <Window.Background>
        <ImageBrush ImageSource="/PSMSUI;component/Images/water-wallpaper.jpg" />
    </Window.Background>
    <Grid Height="327" Width="521">
        <Grid.Background>
            <ImageBrush ImageSource="/PSMSUI;component/Images/water-wallpaper.jpg" />
        </Grid.Background>
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="10,10,0,0" Name="textBlock1" Text="Customer Registration form" VerticalAlignment="Top" Width="457" TextAlignment="Center" FontWeight="Bold" FontSize="18" />
        <Label Content="Full name" Height="24" HorizontalAlignment="Left" Margin="16,51,0,0" Name="label1" VerticalAlignment="Top" Width="82" FontWeight="Bold" />
        <TextBox Height="23" HorizontalAlignment="Right" Margin="0,52,212,0" Name="textBox1" VerticalAlignment="Top" Width="192" BorderBrush="Black">
            <TextBox.Background>
                <SolidColorBrush />
            </TextBox.Background>
        </TextBox>
        <Label Content="Contact No." FontWeight="Bold" Height="24" HorizontalAlignment="Left" Margin="17,87,0,0" Name="label2" VerticalAlignment="Top" Width="82" />
        <TextBox Height="23" HorizontalAlignment="Right" Margin="0,88,211,0" Name="textBox2" VerticalAlignment="Top" Width="192" BorderBrush="Black">
            <TextBox.Background>
                <SolidColorBrush />
            </TextBox.Background>
        </TextBox>
        <Label Content="Address" FontWeight="Bold" Height="24" HorizontalAlignment="Left" Margin="19,125,0,0" Name="label3" VerticalAlignment="Top" Width="82" />
        <TextBox Height="23" HorizontalAlignment="Right" Margin="0,126,212,0" Name="textBox3" VerticalAlignment="Top" Width="192" BorderBrush="Black">
            <TextBox.Background>
                <SolidColorBrush />
            </TextBox.Background>
        </TextBox>
        <Label Content="Gender" FontWeight="Bold" Height="24" HorizontalAlignment="Left" Margin="19,161,0,0" Name="label4" VerticalAlignment="Top" Width="82" />
        <RadioButton Content="Male" Height="16" HorizontalAlignment="Left" Margin="114,167,0,0" Name="radioButton1" VerticalAlignment="Top" FontWeight="Bold" BorderBrush="Black" />
        <RadioButton Content="Female" Height="16" HorizontalAlignment="Left" Margin="178,167,0,0" Name="radioButton2" VerticalAlignment="Top" FontWeight="Bold" BorderBrush="Black" />
        <Label Content="Your income " FontWeight="Bold" Height="24" HorizontalAlignment="Left" Margin="17,197,0,0" Name="label5" VerticalAlignment="Top" Width="82" />
        <TextBox BorderBrush="Black" Height="23" HorizontalAlignment="Right" Margin="0,198,329,0" Name="textBox4" VerticalAlignment="Top" Width="76">
            <TextBox.Background>
                <SolidColorBrush />
            </TextBox.Background>
        </TextBox>
        <ListBox Height="23" HorizontalAlignment="Left" Margin="201,198,0,0" Name="listBox1" VerticalAlignment="Top" Width="51" BorderBrush="Black" DisplayMemberPath="" ItemStringFormat="PA, Daily,Monthly">
            <ListBox.Background>
                <SolidColorBrush />
            </ListBox.Background>
        </ListBox>
        <Button Content="Submit" Height="23" HorizontalAlignment="Left" Margin="116,257,0,0" Name="button1" VerticalAlignment="Top" Width="75" FontWeight="Bold" Background="LightSeaGreen" BorderBrush="Black" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="201,257,0,0" Name="button2" VerticalAlignment="Top" Width="75" FontWeight="Bold" Background="LightSeaGreen" BorderBrush="Black" />
        <Button Content="Reset" Height="23" HorizontalAlignment="Left" Margin="287,257,0,0" Name="button3" VerticalAlignment="Top" Width="75" FontWeight="Bold" Background="LightSeaGreen" BorderBrush="Black" />
    </Grid>
</Window>
